// specialities
.specialities
  ul
    max-width 800px
    margin auto
    padding 0
  li
    list-style none
    lost-column 1/2
    padding 30px 0
    p
      width 60%
      display block
      margin auto
      font-size 1.1em
  h3
    font-weight 100
    lost-column 1/1
    font-size 2rem
    text-align center
    color primary
    lost-utility clearfix
  svg
    width 160px
    height 160px
    display block
    margin auto
