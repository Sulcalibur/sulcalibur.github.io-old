body
  background bg
  color primary

.container
  width 1400px
  margin auto

// Header Section

.logo
  position fixed
  z-index 4
  width 100%
  background-color rgba(70, 98, 159, 1)
  -webkit-clip-path polygon(0 100%,100% 70%,100% 0, 0 0)
  height 414px
  margin-top -320px

header
  position relative
  z-index 5
  background url('../images/bg-temp.jpg')
  background-size cover
  -webkit-clip-path polygon(0 100%,100% 70%,100% 0, 0 0)
  height 414px

nav
  ul
    padding 0
    margin 0
    display block
    padding-top 1rem
    float right
    li
      display inline
      padding 1rem

footer
  background-image:  url('../footer-bg.png')
  background-image none,url('../footer-bg.svg'), url('../footer-bg.png')
  background-size 100% 100%
  min-height 290px
